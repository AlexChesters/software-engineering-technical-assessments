{
  "name": "elections-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npx parcel src/index.html",
    "build": "parcel build",
    "test": "jest --watch"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "babel-preset-react-app": "^10.0.1",
    "jest": "^27.5.1",
    "parcel": "^2.4.1"
  },
  "dependencies": {
    "identity-obj-proxy": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/node_modules/identity-obj-proxy"
    },
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "runtime": "automatic"
        }
      ]
    ]
  }
}
